#include<stdio.h>
void selectionsort(int arr[], int n){
    for(int i=0; i<n-1; i++){
        int min = i;
         for(int j = i+1; j<n; j++){
            if(arr[j] < arr[min]){
                min = j;
            }
         }
         if(min != i){
            int temp = arr[i];
            arr[i] = arr[min];
            arr[min] = temp;
         }
    }
}
int main(){
    int arr[] = {1,7,2,99,34,5};
    int n = sizeof(arr) / sizeof(arr[0]);
    selectionsort(arr,n);
    for(int j=0; j<n; j++){
        printf("%d ",arr[j]);
    }
} 
